{
  "name": "re-ndjson",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "ReNdjsonApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "rely.lib",
        "re-ndjson.lib"
      ],
      "main": "TestReNdjson",
      "name": "TestReNdjson.exe"
    },
    "library": {
      "require": [
        "yojson"
      ],
      "name": "re-ndjson.lib",
      "namespace": "ReNdjson"
    },
    "executable": {
      "require": [
        "cmdliner",
        "re-ndjson.lib"
      ],
      "main": "ReNdjsonApp",
      "name": "ReNdjsonApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "run": "esy x ReNdjsonApp.exe",
    "test": "esy x TestReNdjson.exe",
    "test:watch": "watchman-make -p 'executable/**/*.re' 'library/**/*.re' 'test/**/*.re' --make=esy -t test --settle 1"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/cmdliner": "1.0.3",
    "@opam/dune": ">=1.6.0",
    "@opam/yojson": "1.7.0",
    "@reason-native/rely": "^2.2.0",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
