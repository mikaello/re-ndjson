{
  "name": "re-ndjson",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "ReNdjsonApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "re-ndjson.lib"
      ],
      "main": "TestReNdjson",
      "name": "TestReNdjson.exe"
    },
    "library": {
      "name": "re-ndjson.lib",
      "namespace": "ReNdjson"
    },
    "executable": {
      "require": [
        "cmdliner",
        "re-ndjson.lib"
      ],
      "main": "ReNdjsonApp",
      "name": "ReNdjsonApp.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "run": "esy x ReNdjsonApp.exe",
    "test": "esy x TestReNdjson.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/cmdliner": "1.0.3",
    "@opam/dune": ">=1.6.0",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
